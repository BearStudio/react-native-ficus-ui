{
  "name": "ficus-ui-monorepo",
  "private": true,
  "version": "1.3.3",
  "scripts": {
    "examples": "pnpm -C apps/examples run",
    "examples:web": "pnpm run examples web",
    "dev": "pnpm prepack && pnpm examples:web",
    "prepack": "pnpm -C ./packages/react-native-ficus-ui run prepack",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "typecheck": "pnpm -C ./packages/react-native-ficus-ui run typecheck",
    "test:components": "pnpm -C ./packages/react-native-ficus-ui run test",
    "ci": "pnpm typecheck && pnpm lint && pnpm test:components"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@babel/preset-env": "7.22.20",
    "@babel/preset-react": "7.22.15",
    "@babel/preset-typescript": "7.22.15",
    "@react-native/eslint-config": "0.77.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "eslint": "8.57.1",
    "prettier": "3.4.2"
  },
  "author": {
    "name": "Nicolas Torion",
    "email": "nicolas.torion@bearstudio.fr",
    "url": "https://github.com/ntorionbearstudio"
  },
  "license": "MIT",
  "pnpm": {
    "overrides": {
      "react": "18.3.1",
      "react-native": "0.76.0",
      "react-dom": "18.3.1",
      "@types/react": "18.3.1",
      "@types/react-dom": "18.3.1"
    }
  }
}
