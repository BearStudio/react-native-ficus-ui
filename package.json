{
  "name": "ficus-ui-monorepo",
  "private": true,
  "version": "1.3.3",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "examples": "pnpm -C apps/examples run",
    "examples:web": "pnpm run examples web",
    "build": "pnpm -C ./packages/react-native-ficus-ui run prepack",
    "build:dev": "pnpm build && pnpm examples:web",
    "clean": "rm -rf node_modules packages/react-native-ficus-ui/node_modules packages/react-native-ficus-ui/lib apps/examples/node_modules apps/examples/.expo apps/docs/node_modules",
    "lint": "eslint \"**/*.{js,ts,tsx}\""
  },
  "author": {
    "name": "Nicolas Torion",
    "email": "nicolas.torion@bearstudio.fr",
    "url": "https://github.com/ntorionbearstudio"
  },
  "license": "MIT",
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@babel/preset-env": "7.22.20",
    "@babel/preset-react": "7.22.15",
    "@babel/preset-typescript": "7.22.15",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "metro-react-native-babel-preset": "0.77.0",
    "prettier": "3.4.2"
  },
  "pnpm": {
    "overrides": {
      "react": "18.3.1",
      "react-native": "0.76.0",
      "react-dom": "18.3.1",
      "@types/react": "18.3.1",
      "@types/react-dom": "18.3.1"
    }
  }
}
