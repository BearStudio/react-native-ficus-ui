{
  "name": "ficus-ui-monorepo",
  "private": true,
  "version": "1.3.3",
  "engines": {
    "node": ">= 18.0.0"
  },
  "scripts": {
    "dev": "turbo dev --filter='!./apps/docs'",
    "docs:dev": "pnpm -C ./apps/docs run dev",
    "prepack": "pnpm -C ./packages/components run prepack",
    "lint": "eslint packages --ext .ts,.tsx --config .eslintrc",
    "typecheck": "pnpm -C ./packages/components run typecheck",
    "test:old": "jest --config ./jest.config.cjs",
    "test": "jest --config ./jest.config.cjs --testPathPattern='./packages/components'",
    "ci": "pnpm typecheck && pnpm lint && pnpm test",
    "clean": "del ./**/node_modules ./**/lib ./**/dist ./**/.next ./**/.expo",
    "release": "release-it",
    "format:check": "prettier --check packages --cache",
    "format:fix": "prettier --write packages --cache"
  },
  "dependencies": {
    "@testing-library/react-native": "13.0.1",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/jest": "29.5.2",
    "@types/react-test-renderer": "18.0.0",
    "eslint": "8.57.0",
    "eslint-plugin-testing-library": "7.1.1",
    "prettier": "3.4.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-test-renderer": "18.2.0"
  },
  "devDependencies": {
    "@react-native/eslint-config": "0.77.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "del-cli": "6.0.0",
    "turbo": "2.4.2"
  },
  "author": {
    "name": "Nicolas Torion",
    "email": "nicolas.torion@bearstudio.fr",
    "url": "https://github.com/ntorionbearstudio"
  },
  "license": "MIT",
  "pnpm": {
    "overrides": {
      "react": "18.3.1",
      "react-native": "0.76.0",
      "react-dom": "18.3.1",
      "@types/react": "18.3.1",
      "@types/react-dom": "18.3.1"
    }
  },
  "packageManager": "pnpm@9.15.5"
}
